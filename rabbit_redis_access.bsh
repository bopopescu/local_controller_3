while [ 1 ]
do
   for i in {1..60}
   do
       echo "executing rabbitmq redis gateway $i"
       /usr/bin/python3 rabbit_redis_access_py3.py  2>  /tmp/rabbit_redis_access.err   
     
        mv /tmp/rabbit_redis_access.err /tmp/rabbit_redis_access.errr
        sleep 5
   done
   python3 cloud_event_queue_py3.py "reboot" "rabbit_redis_access"
done
